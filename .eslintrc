{
    "env": {
      "node" : true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
      "max-len": [2, 120, 2],
      "no-var": 2,
      "no-console": 2,
      "prefer-const": 2,
      "object-curly-spacing": [2, "always"],
      "newline-per-chained-call": 2
    }
}
